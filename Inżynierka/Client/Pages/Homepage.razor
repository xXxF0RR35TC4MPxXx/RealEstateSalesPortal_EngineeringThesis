@code {
    bool isLoading = true;
    MatTheme theme = new()
        {
            Primary = "#008000",
            Secondary = "#008000",
            Surface = "#ffffff"
        };

    //top search bar
    private EditContext EditContext;
    private HomepageSearchQueryViewModel model = new()
        {
            estateType = EstateType.APARTMENT,
            offerType = OfferType.SALE
        };
    public static string AddQueryParm(string uri, string parmName, string parmValue)
    {
        var uriBuilder = new UriBuilder(uri);
        var q = System.Web.HttpUtility.ParseQueryString(uriBuilder.Query);
        q[parmName] = parmValue;
        uriBuilder.Query = q.ToString();
        var newUrl = uriBuilder.ToString();
        return newUrl;
    }

    private string Search()
    {
        string offerT, estateT = String.Empty;
        //nawiguj do odpowiedniej podstrony w zależności od pól zaznaczonych w formularzu
        if (model.estateType == EstateType.HOUSE)
        {
            estateT = "House";
        }
        else if (model.estateType == EstateType.APARTMENT)
        {
            estateT = "Apartment";
        }
        else if (model.estateType == EstateType.GARAGE)
        {
            estateT = "Garage";
        }
        else if (model.estateType == EstateType.HALL)
        {
            estateT = "Hall";
        }
        else if (model.estateType == EstateType.ROOM)
        {
            estateT = "Room";
        }
        else if (model.estateType == EstateType.PLOT)
        {
            estateT = "Plot";
        }
        else if (model.estateType == EstateType.PREMISES)
        {
            estateT = "Premises";
        }
        if (model.offerType == OfferType.SALE)
        {
            offerT = "Sale";
        }
        else
        {
            offerT = "Rent";
        }

        string uri = _navigationManager.BaseUri + $"Offer/Search/{estateT}/";
        if (estateT != "Room" && estateT != "Plot")
        {
            uri += $"{offerT}";
        }
        if (model.City != null && model.City != "")
        {
            uri = AddQueryParm(uri, "City", model.City);
        }
        if (model.maxPrice != null && model.maxPrice.Value > 0)
        {
            uri = AddQueryParm(uri, "maxPrice", model.maxPrice.Value.ToString());
        }
        if (model.minPrice != null && model.minPrice.Value >= 0)
        {
            uri = AddQueryParm(uri, "minPrice", model.minPrice.Value.ToString());
        }
        if (model.minArea != null && model.minArea.Value > 0)
        {
            uri = AddQueryParm(uri, "minArea", model.minArea.Value.ToString());
        }
        if (model.maxArea != null && model.maxArea.Value > 0)
        {
            uri = AddQueryParm(uri, "maxArea", model.maxArea.Value.ToString());
        }
        return uri;
    }

    //getting homepage offers here
    public HomepageOffersDTO[]? offers { get; set; }
    public HomepageOffersDTO[]? offersTopRow { get; set; }
    public HomepageOffersDTO[]? offersBottomRow { get; set; }
    protected override async Task OnInitializedAsync()
    {
        EditContext = new(model);

        offers = await Http.GetFromJsonAsync<HomepageOffersDTO[]>("HomeController/Get");
        offersTopRow = offers.Take(offers.Length / 2).ToArray();
        offersBottomRow = offers.Skip(offers.Length / 2).ToArray();
        isLoading = false;
        StateHasChanged();
        await base.OnInitializedAsync();
    }
    public string? rooms { get; set; }


    //helpful links logic
    public string[] states { get; set; } = new string[7] {"d-show", "d-none", "d-none","d-none","d-none","d-none","d-none" };
    public async Task onclick(int i)
    {
        for(int j=0; j<states.Count();j++)
        {
            states[j] = "d-none";
        }
        states[i] = "d-show";
    }

}
