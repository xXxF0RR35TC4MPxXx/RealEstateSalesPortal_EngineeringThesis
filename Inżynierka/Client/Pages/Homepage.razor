<div id="topBar">
    <img id="topBarImage" src="../images/back.jpg" />
    <legend id="legend">O jakim miejscu zamieszkania marzysz?</legend>
    <div id="topBannerAndSearchForm">
        <EditForm id="editForm" EditContext="@EditContext">
            <div class="indexInput">
                <InputSelect class="searchInputSelect" @bind-Value="model.estateType">
                    @foreach (var estateType in Enum.GetValues(typeof(EstateType)))
                    {
                        <option value="@estateType">@EnumHelper.GetDescriptionFromEnum((EstateType)estateType)</option>
                    }
                </InputSelect>

            </div>
            <div class="indexInput">
                <input placeholder="Powierzchnia min." @bind="model.minArea" type="number" min=0 step=1 class="form-control" id="txtminArea" />
            </div>
            <div class="indexInput">
                <input placeholder="Cena min." @bind="model.minPrice" type="number" min=0 step=1 class="form-control" id="txtminPrice" />
            </div>
            <div class="indexInput">
                <input placeholder="Miasto" @bind="model.City" type="text" class="form-control" id="txtCity" />
            </div>
            <div class="indexInput">
                <InputSelect class="searchInputSelect" @bind-Value="model.offerType">
                    @foreach (var offerType in Enum.GetValues(typeof(OfferType)))
                    {
                        <option value="@offerType">@EnumHelper.GetDescriptionFromEnum((OfferType)@offerType)</option>
                    }
                </InputSelect>

            </div>
            <div class="indexInput">
                <input placeholder="Powierzchnia max." @bind="model.maxArea" type="number" min=0 step=1 class="form-control" id="txtmaxArea" />
            </div>
            <div class="indexInput">
                <input placeholder="Cena max." @bind="model.maxPrice" type="number" min=0 step=1 class="form-control" id="txtmaxPrice" />
            </div>
            <MatThemeProvider Theme=@theme>
                <MatButtonLink style="margin-top: 7px;" Unelevated="true" Icon="search" Href="@Search()">Szukaj</MatButtonLink>
            </MatThemeProvider>
        </EditForm>
    </div>
</div>

<!--osiem najnowszych ofert użytkowników-->
<div class="homepageOffers">
    @{
        <SpinLoader IsLoading="isLoading">
            <LoadingTemplate>
                <div>
                    <br /><br />
                    <CircleFade Center="true" size="70px"></CircleFade>
                </div>
            </LoadingTemplate>
            <ContentTemplate>
                @if (offersTopRow is not null && offersTopRow.Any())
                {
                    <div class="mainHeaders">Ostatnio dodane oferty</div>
                    <div class="wrap">
                        @foreach (var offer in offersTopRow)
                        {
                            string imgSrc = "";
                            if(offer.Photo!=null)
                            {
                                string base64 = Convert.ToBase64String(@offer.Photo);
                                imgSrc = String.Format("data:image/jpg;base64,{0}", base64);
                            }
                            else
                            {
                                imgSrc = "../images/placeholder.jpg";
                            }
                            <div class="tile">
                                <a href="Offer/@offer.Id">
                                    <img src='@imgSrc' />
                                    <div class="text">
                                        <h1 style="font-size: 24px;">@offer.OfferTitle </h1>
                                        <h2 class="animate-text">
                                            @Decimal.ToInt32(offer.Price)zł @if (offer.ForSaleOrForRent.ToLower() == "wynajem")
                                            {
                                                <span> / msc</span>
                                            }
                                        </h2>
                                        <p class="animate-text">
                                            @offer.OfferCategory na @offer.ForSaleOrForRent.ToLower() w @offer.City
                                            @if (offer.Area != null)
                                            {
                                                <span> | @offer.Area m<sup>2</sup></span>
                                            }
                                            @if (offer.Area != null && offer.RoomCount != null)
                                            {
                                                <span> | </span>
                                            }
                                            @if (offer.RoomCount != null)
                                            {
                                                <span>@offer.RoomCount @PolishConjugationHelper.GetRoomWord(@offer.RoomCount)</span>
                                            }
                                        </p>
                                    </div>
                                </a>
                            </div>
                        }
                    </div>
                }
                @if (offersBottomRow is not null && offersBottomRow.Any())
                {
                    <div class="wrap" style="padding-bottom: 30px;">
                        @foreach (var offer in offersBottomRow)
                        {

                            var base64 = (offer.Photo!=null) ? Convert.ToBase64String(offer.Photo) : "";
                            var imgSrc = String.Format("data:image/jpg;base64,{0}", base64);

                            <div class="tile">
                                <a href="Offer/@offer.Id">
                                    @if(offer.Photo!=null)
                                    {
                                        <img src='@imgSrc' />
                                    }
                                    else
                                    {
                                        <img src='./images/placeholder.jpg'>
                                    }
                                    <div class="text">
                                        <h1 style="font-size: 24px;">@offer.OfferTitle </h1>
                                        <h2 class="animate-text">
                                            @Decimal.ToInt32(offer.Price)zł @if (offer.ForSaleOrForRent.ToLower() == "wynajem")
                                            {
                                                <span> / msc</span>
                                            }
                                        </h2>
                                        <p class="animate-text">
                                            @offer.OfferCategory na @offer.ForSaleOrForRent.ToLower() w @offer.City
                                            @if (offer.Area != null)
                                            {
                                                <span> | @offer.Area m<sup>2</sup></span>
                                            }
                                            @if (offer.RoomCount != null)
                                            {
                                                <span> | @offer.RoomCount @PolishConjugationHelper.GetRoomWord(@offer.RoomCount)</span>
                                            }
                                        </p>
                                    </div>
                                </a>
                            </div>
                        }
                    </div>
                }
            </ContentTemplate>
        </SpinLoader>
    }
</div>

