    .tile
    {
    font-family: Arial, sans-serif;
    border-radius: 5px;
    width:380px;
    height:240px;
    margin:10px;
    font-weight: 700;
    background-color:#717999;
    display:inline-block;
    background-size:cover;
    position:relative;
    cursor:pointer;
    transition: all 0.4s ease-out;
    box-shadow: 0px 35px 77px -17px rgba(0,0,0,0.44);
    overflow:hidden;
    color:white;
    }

    .tile img
    {
    height:100%;
    width:100%;
    position:absolute;
    object-fit: cover;
    overflow: hidden;
    top:0;
    left:0;
    z-index:0;
    transition: all 0.4s ease-out;
    }

    .tile .text
    {
    /*   z-index:99; */
    position:absolute;
    padding:15px;
    height:calc(100% - 60px);
    }

    .tile a:hover{
    color: white;
    }

    .tile h1
    {
    font-size: 24;
    font-weight:700;
    margin:0;
    text-shadow: -1px -1px 0 #000, 1px -1px 0 #000, -1px 1px 0 #000, 1px 1px 0 #000;
    }

    .tile h2
    {
    font-weight:100;
    margin:20px 0 0 0;
    font-style:italic;
    transform: translateX(200px);
    }

    .tile p
    {
    font-weight:300;
    margin:20px 0 0 0;
    line-height: 25px;
    /*   opacity:0; */
    transform: translateX(-200px);
    transition-delay: 0.2s;
    }

    .animate-text
    {
    opacity:0;
    margin-right: 0px;
    transition: all 0.6s ease-in-out;
    word-wrap: break-word;
    text-shadow: -1px -1px 0 #000, 1px -1px 0 #000, -1px 1px 0 #000, 1px 1px 0 #000;
    }

    .tile:hover
    {
    /*   background-color:#99aeff; */
    box-shadow: 0px 35px 77px -17px rgba(0,0,0,0.64);
    transform:scale(1.05);
    }

    .tile:hover img
    {
    opacity: 0.2;
    }

    .tile:hover .animate-text
    {
    transform:translateX(0);
    opacity:1;
    }

    .tile:hover span
    {
    opacity:1;
    transform:translateY(0px);
    }

